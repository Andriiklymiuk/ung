    "test": "node ./out/test/runTest.js",
    "test:cov": "node ./out/test/runTest.js",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format --write ./src",
    "prepare": "lefthook install"
  },
