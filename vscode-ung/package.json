    "configuration": {
      "title": "UNG",
      "properties": {
        "ung.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh views after operations"
        },
        "ung.showStatusBar": {
          "type": "boolean",
          "default": true,
          "description": "Show active time tracking in status bar"
        },
        "ung.roundHours": {
